{
  "name": "jest-test",
  "version": "1.0.0",
  "description": "Test enzyme-to-json integration in Jest tested apps\"",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "updateSnapshot": "jest --updateSnapshot",
    "build": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bengrunfeld/jest-test.git"
  },
  "keywords": [
    "jest",
    "test",
    "enzyme",
    "snapshot"
  ],
  "author": "bengrunfeld",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bengrunfeld/jest-test/issues"
  },
  "homepage": "https://github.com/bengrunfeld/jest-test#readme",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  },
  "jest": {
    "setupFiles": [
      "./setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  }
}
